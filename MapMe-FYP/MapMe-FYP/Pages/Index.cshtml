@page
@model IndexModel
@{
    ViewData["Title"] = "MapMe";
}
<h2>@ViewData["Title"]</h2>

<!DOCTYPE html>
<html>
<head>
    <style>
        #map {
            height: 750px;
            width: 100%;
        }
    </style>
</head>
<body>
    <div id="map"></div>
    <script>
        function initMap() {
            var map = new google.maps.Map(document.getElementById('map'), {
                zoom: 6,
                center: new google.maps.LatLng(54.9, -2.5),
            });
            var tees = { lat: 54.570755, lng: -1.235222 };
            var marker = new google.maps.Marker({
                position: tees,
                map: map
            });
        }
    </script>
    <script async defer
            src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBBDW3-0bPIBFdW7_YkLukWk9s8A_908Mw&callback=initMap">
    </script>
</body>
</html>

